#   print(1)
def promedio(a):
  for p in a:
    if (a !=0):
      break
  print("1")
  
  promedio = sum(a)/len(a)
  print("el promedio es ", promedio)
promedio(a)

def reprobaron(a):
  for r in a:
    if (a >= [6]):
      break
  print("1")
reprobaron(a)
